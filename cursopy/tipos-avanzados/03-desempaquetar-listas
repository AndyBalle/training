numeros = [1, 2, 3, 4, 5, 6 ,7, 8, 9]

""" #este codigo abajop feo 
primero = numeros[0]
segundo = numeros[1]
tercero = numeros[2] """
#mejor
primero, segundo, *otros = numeros  # aca ocurre el desempaquetado 
print(primero, segundo, otros)

primero, segundo, *otros, ultimo = numeros  # aca ocurre el desempaquetado 
print(primero, ultimo, otros)

primero, segundo, *otros, penu, ultimo = numeros  # aca ocurre el desempaquetado 
print(segundo, penu, otros)